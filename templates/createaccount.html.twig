{% extends "master.html.twig" %}

{% block content %}

{% if errorList %}
    <ul>
    {% for error in errorList %}
        <li>{{ error }}</li>
    {% endfor %}
    </ul>
{% endif %}

<form method="post">
    Email <input type="email" name="email" value="{{v.semail}}"><br/>
    Password <input type="password" name="pass1" value="{{v.sname}}"><br/>
    Confirm Password <input type="password" name="pass2" value="{{v.sname}}"><br/>
    <input type="submit" value="Create Account">
</form>


{% endblock content %}