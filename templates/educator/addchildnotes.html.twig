{% extends "educator/master.html.twig" %}


{% block content %}
{% if errorList %}
    <ul>
    {% for error in errorList %}
        <li>{{ error }}</li>
    {% endfor %}
    </ul>
{% endif %}
<h1 class ="edu"><img src="/images/child.jpg" width="70px"/>Add Child Note</h1>
   <form class="form" method="post"enctype="multipart/form-data" >

   <input type="text"hidden name="childId"  value="{{child.id}}"><br/>
    First Name: <input class ="note"type="text" readonly name="firstName" value="{{child.firstName}}"><br/>
    Last Name: <input class ="note" type="text" readonly name="lastName" value="{{child.lastName}}"><br/>
    Image: <input class ="note" type="file" name="image"><br>
    Height(cm): <input class ="note" type="textr" name="height"><br>
    Weight(kg): <input class ="note" type="text" name="weight"><br>
    Skill: <input class ="note" type="text" name="skills" ><br>
    Notes: <textarea class ="note" name="note"></textarea><br>
    <input class ="submitList1"  type="submit" value="submit">
</form> 
{% endblock content %}
 