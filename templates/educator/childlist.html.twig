{% extends "master.html.twig" %}

{% block content %}

<h3>Group Child List:</h3>

    <table>
        <tr><th>First Name</th><th>Last Name</th><th>Gender</th><th>Date Of Birth</th><th>EmerContactName</th><th>EmergencyPhone</th></tr>
        {% for a in list %}
        <tr>
        <td>{{a.firstName}}</td>
        <td>{{a.lastName}}</td>
        <td>{{a.gender}}</td>
        <td>{{a.dateOfBirth}}</td>
        <td>{{a.emergencyContact}}</td>
        <td>{{a.emergencyPhone}}</td>
        
        </tr>
        {% endfor %}
    </table>

{% endblock content %}