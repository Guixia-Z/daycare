{% extends "master.html.twig" %}

{% block content %}


<a href="/educator/childlist">Go Back</a> </p>

<table>
    <tr><th>First Name </th><th>Last Name</th> <th>Photo</th> <th>Height(cm)</th> <th>Weight(kg)</th><th>Skill</th> <th>Notes</th><th>Record Time</th></tr>
    {% for a in list %}
        <tr>
            <td>{{a.firstName}}</td>
            <td>{{a.lastName}}</td>
            <td><img src="../{{a.photoFilePath}}" width="100"></td>
            <td>{{a.height}}</td>
            <td>{{a.weight}}</td>
            <td>{{a.skills}}</td>
            <td>{{a.note}}</td>
             <td>{{a.noteCreatedTS}}</td>
           
            
        </tr>
    {% endfor %}
</table>

{% endblock content %}

