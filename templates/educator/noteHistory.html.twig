{% extends "master.html.twig" %}

{% block content %}


<a href="/educator/addchildnotes?childId={{childId}}">Add Notes</a> </p>
Date: {{today}}<br>

<table>
    <tr> <th>First Name </th> <th>Last Name</th> <th>Photo</th> <th>Height(cm)</th> <th>Weight(kg)</th> <th>Skill</th> <th>Notes</th> <th>Record Time</th> </tr>
    {% for a in list %}
        <tr>
            <td>{{a.firstName}}</td>
            <td>{{a.lastName}}</td>
           <td><img src="../{{a.photoFilePath}}" width="150" alt=""></td>
            <td>{{a.height}}</td>
            <td>{{a.weight}}</td>
            <td>{{a.skills}}</td>
            <td>{{a.note}}</td>
             <td>{{a.noteCreatedTS}}</td>
            <td><a href="/educator/note_edit?noteId={{a.id}}"> Edit</a></td>
            <td><a href="/educator/note_remove?noteId={{a.id}}">Remove</a></td>
        </tr>
    {% endfor %}
</table>
<a href="/educator/childlist">Go Back</a><br>
{% endblock content %}
