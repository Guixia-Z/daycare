{% extends "master.html.twig" %}

{% block title %}Waitinglist{% endblock %}

{% block content %}

<table>
    <tr><th>First Name</th><th>Last Name</th><th>Gender</th><th>Date of Birth</th><th>Has Siblings</th></tr>
    {% for a in list %}
        <tr>
            <td>{{a.firstName}}</td>
            <td>{{a.lastName}}</td>
            <td>{{a.gender}}</td>
            <td>{{a.dateOfBirth}}</td>
            <td>{{a.hasSibling}}</td>
        </tr>
    {% endfor %}
</table>

<p><a href="/parent/parent_board">Go back to parent account page</a>. </p>

{% endblock %}